{
  "name": "acougue-digital",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "codegen": "graphql-codegen --config codegen.yml",
    "schema:download": "apollo schema:download --endpoint=http://localhost:4000/graphql",
    "codegen:generate": "apollo codegen:generate --queries=./src/**/*.tsx --schema=./schema.json --outputFlat --target=typescript ./src/schemaTypes.ts",
    "gen:types": "npm run schema:download && npm run codegen:generate"
  },
  "dependencies": {
    "@apollo/client": "3.6.9",
    "@graphql-codegen/cli": "^2.6.2",
    "@chakra-ui/react": "^2.2.9",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.6",
    "apollo-boost": "^0.4.9",
    "apollo-link-error": "^1.1.13",
    "formik": "^2.2.9",
    "framer-motion": "^7.2.1",
    "graphql": "^16.6.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-icons": "^4.4.0",
    "react-input-mask": "^2.0.4",
    "react-router-dom": "^6.3.0",
    "stripe": "^10.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-operations": "2.5.3",
    "@graphql-codegen/typescript-react-apollo": "3.3.3",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-input-mask": "^1.2.1",
    "@vitejs/plugin-react": "^1.3.0",
    "autoprefixer": "^10.4.7",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.4",
    "typescript": "^4.6.3",
    "vite": "^2.9.9"
  }
}